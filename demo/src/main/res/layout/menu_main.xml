<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/linearLayout7"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:background="#fcfcfc">

    <ImageView
        android:id="@+id/fctlogo"
        android:layout_width="0dp"
        android:layout_height="181dp"
        android:layout_marginTop="64dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/fctletras" />

    <ImageView
        android:id="@+id/infobutton"
        android:layout_width="0dp"
        android:layout_height="92dp"
        android:layout_marginStart="8dp"
        android:layout_marginLeft="8dp"
        android:layout_marginTop="100dp"
        android:layout_marginEnd="2dp"
        android:layout_marginRight="2dp"
        app:layout_constraintEnd_toStartOf="@+id/calbutton"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/fctlogo"
        app:srcCompat="@drawable/info2" />

    <ImageView
        android:id="@+id/calbutton"
        android:layout_width="0dp"
        android:layout_height="92dp"
        android:layout_marginStart="2dp"
        android:layout_marginLeft="2dp"
        android:layout_marginTop="100dp"
        android:layout_marginEnd="2dp"
        android:layout_marginRight="2dp"
        app:layout_constraintEnd_toStartOf="@+id/mapbutton"
        app:layout_constraintStart_toEndOf="@+id/infobutton"
        app:layout_constraintTop_toBottomOf="@+id/fctlogo"
        app:srcCompat="@drawable/schedule3" />

    <ImageView
        android:id="@+id/mapbutton"
        android:layout_width="0dp"
        android:layout_height="92dp"
        android:layout_marginStart="2dp"
        android:layout_marginLeft="2dp"
        android:layout_marginTop="100dp"
        android:layout_marginEnd="8dp"
        android:layout_marginRight="8dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/calbutton"
        app:layout_constraintTop_toBottomOf="@+id/fctlogo"
        app:srcCompat="@drawable/mapa1" />

    <ImageView
        android:id="@+id/noticiasbutton"
        android:layout_width="0dp"
        android:layout_height="92dp"
        android:layout_marginStart="8dp"
        android:layout_marginLeft="8dp"
        android:layout_marginTop="32dp"
        android:layout_marginEnd="2dp"
        android:layout_marginRight="2dp"
        app:layout_constraintEnd_toStartOf="@+id/happymealbutton"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView4"
        app:srcCompat="@drawable/news1" />

    <ImageView
        android:id="@+id/happymealbutton"
        android:layout_width="0dp"
        android:layout_height="92dp"
        android:layout_marginStart="8dp"
        android:layout_marginLeft="8dp"
        android:layout_marginTop="32dp"
        android:layout_marginEnd="2dp"
        android:layout_marginRight="2dp"
        app:layout_constraintEnd_toStartOf="@+id/teambutton"
        app:layout_constraintStart_toEndOf="@+id/noticiasbutton"
        app:layout_constraintTop_toBottomOf="@+id/textView5"
        app:srcCompat="@drawable/hm" />

    <ImageView
        android:id="@+id/teambutton"
        android:layout_width="0dp"
        android:layout_height="92dp"
        android:layout_marginStart="2dp"
        android:layout_marginLeft="2dp"
        android:layout_marginTop="32dp"
        android:layout_marginEnd="8dp"
        android:layout_marginRight="8dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/happymealbutton"
        app:layout_constraintTop_toBottomOf="@+id/textView6"
        app:srcCompat="@drawable/team2" />

    <TextView
        android:id="@+id/textView4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="16dp"
        android:text="Informações"
        android:textColor="#000"
        android:textSize="17sp"
        app:layout_constraintEnd_toEndOf="@+id/infobutton"
        app:layout_constraintStart_toStartOf="@+id/infobutton"
        app:layout_constraintTop_toBottomOf="@+id/infobutton" />

    <TextView
        android:id="@+id/textView5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="16dp"
        android:text="Calendário"
        android:textColor="#000"
        android:textSize="17sp"
        app:layout_constraintEnd_toEndOf="@+id/calbutton"
        app:layout_constraintStart_toStartOf="@+id/calbutton"
        app:layout_constraintTop_toBottomOf="@+id/calbutton" />

    <TextView
        android:id="@+id/textView6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="16dp"
        android:text="Mapa"
        android:textColor="#000"
        android:textSize="17sp"
        app:layout_constraintEnd_toEndOf="@+id/mapbutton"
        app:layout_constraintStart_toStartOf="@+id/mapbutton"
        app:layout_constraintTop_toBottomOf="@+id/mapbutton" />

    <TextView
        android:id="@+id/textView7"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="16dp"
        android:text="Notícias"
        android:textColor="#000"
        android:textSize="17sp"
        app:layout_constraintEnd_toEndOf="@+id/noticiasbutton"
        app:layout_constraintStart_toStartOf="@+id/noticiasbutton"
        app:layout_constraintTop_toBottomOf="@+id/noticiasbutton" />

    <TextView
        android:id="@+id/textView8"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="16dp"
        android:text="HappyMeal"
        android:textColor="#000"
        android:textSize="17sp"
        app:layout_constraintEnd_toEndOf="@+id/happymealbutton"
        app:layout_constraintStart_toStartOf="@+id/happymealbutton"
        app:layout_constraintTop_toBottomOf="@+id/happymealbutton" />

    <TextView
        android:id="@+id/textView9"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="16dp"
        android:text="Equipa"
        android:textColor="#000"
        android:textSize="17sp"
        app:layout_constraintEnd_toEndOf="@+id/teambutton"
        app:layout_constraintStart_toStartOf="@+id/teambutton"
        app:layout_constraintTop_toBottomOf="@+id/teambutton" />
</androidx.constraintlayout.widget.ConstraintLayout>